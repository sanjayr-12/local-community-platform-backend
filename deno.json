{
  "nodeModulesDir":"auto",
  "imports": {
    "drizzle-orm": "npm:drizzle-orm@^0.45.1",
    "hono": "jsr:@hono/hono@^4.11.7",
    "postgres": "npm:postgres@^3.4.8"
  },
  "tasks": {
    "start": "deno run --allow-net src/main.ts",
    "dev": "deno run --watch --env --allow-net --allow-env src/main.ts",
    "gen-migration": "npx drizzle-kit generate",
    "run-migration": "npx drizzle-kit migrate"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
